{
    "name": "Mark URL",
    "manifest_version": 2,
    "version": "0.2",
    "description": "Mark the current url by click in extension popup",
    "browser_action": {
    "default_icon": "ext1.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ]
}